	can = commit --amend --no-edit
	olog = log --pretty=format:"%C(cyan)%cD%Creset|%Cred%t%Creset|%Cgreen%s%Creset|%C(yellow)%cn"
	lg = log -p
	co = checkout
	br = branch
	dc = diff --cached
	lgpo = log --pretty=oneline
	lgo = log --oneline